{
  "welcomeFile": "rmwebshoptest/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^/sap/(.*)$",
      "target": "/sap/$1",
      "destination": "DWS300_HTTP",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    }
  ]
}


